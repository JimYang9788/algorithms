    "def isValidSubsequence(array, sequence):\n",
    "    # Write your code here.\n",
    "    arrIdx = 0 \n",
    "    seqIdx = 0\n",
    "\n",
    "    while arrIdx < len(array) and seqIdx < len(sequence):\n",
    "        if array[arrIdx] == sequence[seqIdx]:\n",
    "            seqIdx += 1 \n",
    "        arrIdx += 1 \n",
    "    return seqIdx == len(sequence)"
