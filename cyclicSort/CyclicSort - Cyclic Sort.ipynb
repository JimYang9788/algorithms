   "source": [
    "def cyclic_sort(nums):\n",
    "    i = 0\n",
    "    while i < len(nums):\n",
    "    j = nums[i] - 1\n",
    "    if nums[i] != nums[j]:\n",
    "        nums[i], nums[j] = nums[j], nums[i]  # swap\n",
    "    else:\n",
    "        i += 1\n",
    "    return nums\n"
   ]
  }
