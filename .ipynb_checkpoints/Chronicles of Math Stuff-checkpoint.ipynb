    "class Solution:\n",
    "    def consecutiveNumbersSum(self, n: int) -> int:\n",
    "        count = 0 \n",
    "        upper = ceil((2 * n + 0.25) ** 0.5 - 0.5) + 1\n",
    "        for k in range (1, upper):\n",
    "            if (n - (k + 1) * k // 2) % k == 0:\n",
    "                count += 1 \n",
    "        return count "
   ]
