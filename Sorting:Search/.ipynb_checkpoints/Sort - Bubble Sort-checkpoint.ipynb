    "def bubbleSort(array):\n",
    "\n",
    "\t\n",
    "\tdef bubblePop (array):\n",
    "\t\ti = 0 \n",
    "\t\twhile i < len(array) - 1:\n",
    "\t\t\tif array[i+1] < array[i]:\n",
    "\t\t\t\tarray[i+1], array[i] = array[i], array[i+1]\n",
    "\t\t\ti += 1 \n",
    "\t\n",
    "\tfor i in range(len(array)):\n",
    "\t\tbubblePop (array)\n",
    "\t\t\n",
    "\treturn array \n",
    "\t\t"
