    "from math import e, log\n",
    "class Solution:\n",
    "    def mySqrt(self, x):\n",
    "        if x < 2:\n",
    "            return x\n",
    "        \n",
    "        left = int(e**(0.5 * log(x)))\n",
    "        right = left + 1\n",
    "        return left if right * right > x else right"
   ]
