   "source": [
    "def remove_duplicate_inplace (arr):\n",
    "    i = 1 \n",
    "    j = 1 \n",
    "    \n",
    "    while i < len(arr):\n",
    "        if arr[i] != arr[i-1]:\n",
    "            arr[j] = arr[i]\n",
    "            j += 1 \n",
    "        i += 1 \n",
    "    \n",
    "    return arr[:j]\n"
   ]
  },
